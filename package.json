{
  "private": true,
  "scripts": {
    "start": "umi dev",
    "build": "umi build",
    "postinstall": "umi generate tmp",
    "prettier": "prettier --write '**/*.{js,jsx,tsx,ts,less,md,json}'",
    "test": "umi-test",
    "test:coverage": "umi-test --coverage",
    "dev": "umi dev"
  },
  "gitHooks": {
    "pre-commit": "lint-staged"
  },
  "lint-staged": {
    "*.{js,jsx,less,md,json}": [
      "prettier --write"
    ],
    "*.ts?(x)": [
      "prettier --parser=typescript --write"
    ]
  },
  "dependencies": {
    "@ant-design/icons": "^4.3.0",
    "@ant-design/pro-layout": "^5.0.12",
    "@umijs/preset-react": "^1.7.0",
    "@umijs/test": "^3.2.28",
    "antd": "^4.8.0",
    "axios": "^0.21.0",
    "echarts": "^4.9.0",
    "echarts-for-react": "^2.0.16",
    "js-md5": "^0.7.3",
    "lint-staged": "^10.0.7",
    "moment": "^2.29.1",
    "prettier": "^1.19.1",
    "react": "^16.12.0",
    "react-dom": "^16.12.0",
    "react-dropzone": "^11.2.4",
    "react-helmet-async": "^1.0.4",
    "umi": "^3.2.28",
    "umi-request": "^1.0.8",
    "yorkie": "^2.0.0"
  }
}
